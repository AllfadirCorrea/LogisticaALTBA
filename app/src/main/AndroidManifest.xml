<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.logisticaaltba">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_altba"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_altba_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.LogisticaALTBA"
            android:usesCleartextTraffic="true">
        <activity android:name=".EntregarPedidosAct" android:parentActivityName=".Dashboard">

        </activity>
        <activity android:name=".RecibirPedidosAct" android:parentActivityName=".Dashboard">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard" android:parentActivityName=".MainActivity">
            <!--The meta-data tag is required if you support API level 15 and lower
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".MainActivity" />-->
        </activity>
    </application>

</manifest>